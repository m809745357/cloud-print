{
  "name": "pkg6/cloud-print",
  "license": "MIT",
  "description": "非官方云小票机SDK，支持飞鹅云，芯烨云，易联云，快递100，映美云，中午云，佳博云，优声云，365智能云打印等",
  "keywords": [
    "Cloud Print",
    "云打印机",
    "云小票机",
    "飞鹅云",
    "芯烨云",
    "易联云",
    "快递100",
    "映美云",
    "佳博云",
    "365智能云打印",
    "中午云",
    "优声云"
  ],
  "authors": [
    {
      "name": "zhiqiang",
      "email": "zhiqiang2033@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-zlib": "*",
    "psr/cache": "~1.0|^2.0",
    "psr/log": "^1.0.1 || ^2.0",
    "psr/simple-cache": "^1.0|^2.0",
    "guzzlehttp/guzzle": "^6|^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^7|^8|^9",
    "mockery/mockery": "^1.2.3",
    "friendsofphp/php-cs-fixer": "^3.9",
    "pkg6/log": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Pkg6\\CloudPrint\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pkg6\\CloudPrint\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fresh": [
      "rm -rf vendor composer.lock",
      "composer install"
    ],
    "tests": [
      "php ./vendor/bin/phpunit"
    ],
    "fixer": "vendor/bin/php-cs-fixer fix",
    "try-fixer": "vendor/bin/php-cs-fixer fix --diff --dry-run"
  }
}
